class M
{
	
	void add(int a)//method overloading with single parameter
	{
      int s=a;
      System.out.println(s);
      
	}
	void add(int a,int b)//method overloading with 2 parameter
	{
		int p=a;
		int q=b;
		System.out.println(p+q);
	}
	void add(float a,float b)//method overloading with float type
	{
		float t=a;
		float u=b;
		System.out.println(t+u);
	}
}

public class Mathematics {

	public static void main(String[] args) {
		
		// TODO Auto-generated method stub
 M m=new M();
 m.add(2,3);
 m.add(2);
 m.add(2.342f,12.22f);
 
 
	}

}
